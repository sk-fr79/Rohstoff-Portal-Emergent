		,#BASETABLENAME#_DELETE( 	ENUM_VALID_THEME.#BASETABLENAME#, 	ENUM_VALID_BASICFUNCTION.DELETE)
		,#BASETABLENAME#_ATTACHMENT( 	ENUM_VALID_THEME.#BASETABLENAME#, 	ENUM_VALID_BASICFUNCTION.ATTACHMENT)
    * 
    */
	
	EDIT(ENUM_VALIDATION.#BASETABLENAME#_EDIT)
	,VIEW(ENUM_VALIDATION.#BASETABLENAME#_VIEW)
	,NEW(ENUM_VALIDATION.#BASETABLENAME#_NEW)
	,DELETE(ENUM_VALIDATION.#BASETABLENAME#_DELETE)
	,ATTACHMENT(ENUM_VALIDATION.#BASETABLENAME#_ATTACHMENT)
	;
	
	private ENUM_VALIDATION m_validation = null;
 
	private #PREFIX#_VALIDATORS(ENUM_VALIDATION validation) {
		this.m_validation = validation; 
	}
	
	public ENUM_VALIDATION getValidation() {
		return m_validation;
	}
 
	public E2_ButtonAUTHValidator getValidator() {
		return this.m_validation.getValidator();
	}
 
 
 
}

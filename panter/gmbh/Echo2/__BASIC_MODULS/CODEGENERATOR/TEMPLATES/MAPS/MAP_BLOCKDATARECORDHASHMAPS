 	public MyDataRecordHashMap get_MyDataRecordHashMap_#TABLEBASE#(int iPos) throws myException 
 	{ 
 		if (iPos<0) 
 			throw new myException(this,"Number MUST be greater equal 0 "); 
  
 		if (iPos>=this.vIDs.size()) 
 			throw new myException(this,"Number of Row smaller than "+iPos); 
  
 		String cID = this.vIDs.get(iPos); 
 		return this.oHM_aus_MyDataRecordHashMaps.get(cID); 
 	} 
  

 	public MyDataRecordHashMap get_MyDataRecordHashMap_#TABLEBASE#(String cID) throws myException 

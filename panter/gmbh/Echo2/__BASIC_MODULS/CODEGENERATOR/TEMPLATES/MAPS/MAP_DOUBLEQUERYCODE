		} 
	}



	public Double get_D_#FIELDNAME#(int iPos, Double dValueWhenNULL) throws myException
	{
		if (iPos<0)
			throw new myException(this,"Number MUST be greater equal 0 ");

		if (iPos>=this.vIDs.size())
			throw new myException(this,"Number of Row smaller than "+iPos);

		String cID = this.vIDs.get(iPos);

		Double dRueck =  this.oHM_aus_MyDataRecordHashMaps.get(cID).get_doubleValue("#FIELDNAME#");
		if (dRueck==null) 
		{ 
			return dValueWhenNULL; 
		} 
		else 
		{ 
			return dRueck;
		} 
	}



	public Double get_D_#FIELDNAME#(String cID, Double dValueWhenNULL) throws myException
	{

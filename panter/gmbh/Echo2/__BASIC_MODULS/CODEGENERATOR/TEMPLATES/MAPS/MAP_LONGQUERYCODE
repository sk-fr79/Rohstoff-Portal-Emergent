	public Long get_L_#FIELDNAME#(int iPos, Long lValueWhenNULL) throws myException
	{
		if (iPos<0)
			throw new myException(this,"Number MUST be greater equal 0 ");

		if (iPos>=this.vIDs.size())
			throw new myException(this,"Number of Row smaller than "+iPos);

		String cID = this.vIDs.get(iPos);
		Long lRueck =  this.oHM_aus_MyDataRecordHashMaps.get(cID).get_longValue("#FIELDNAME#");
